.talk-meta {
  dt {
    color: #999;
    font-weight: normal;
    margin: 15px 0 0;
  }
  dd {
    font-size: 28px;
  }
}
.small-title {
  font-size: 16px;
  font-weight: normal;
  color: #999;
}

.schedule-table {
  font-size: 16px;
  thead {
    th {
      color: #999;

    }
  }
  th {
    color: $dark-blue;
    font-weight: normal;
  }
}

.speaker-link {
  margin: 0;
  a {
    color: $dark-blue;
    font-size: 0.8em;
  }
}